        <h1 class="display-4"><i class="fas fa-cloud"></i>AssOnCloud</h1>
        <p class="lead">All your assignments in one place!</p>
        <hr class="4">
    </div>
    
    <div class="conatiner">
        <div class="row">
            <div class="col-lg-12">
                <div id="content">
                    <h1>Please Select Divison For <%= info.branch %> Year: <%= info.year %></h1>
                    <a class="btn btn-outline-info" href="/<%= info.baseUrl%>/<%= info.year%>/assignments?div=A" role="button">Division A</a>
                    <a class="btn btn-outline-info" href="/<%= info.baseUrl%>/<%= info.year%>/assignments?div=B" role="button">Divison B</a>
                    <a class="btn btn-outline-info" href="/<%= info.baseUrl%>/<%= info.year%>/assignments?div=C" role="button">Divison C</a> 
                </div>
            </div>
        </div>
    </div>
<% include partials/footer %>